{
  "name": "akeneo-design-system",
  "version": "0.0.1",
  "description": "Akeneo design system",
  "main": "src/index.ts",
  "scripts": {
      "prebuild": "rimraf lib && yarn test:run && yarn lint:check && yarn ts-build",
      "ts-build": "tsc -p ./tsconfig.build.json --emitDeclarationOnly",
      "check-types": "tsc",
      "check-types:watch": "yarn check-types -- --watch",
      "test:run": "echo jest",
      "test:watch": "echo 'jest --watch'",
      "lint:fix": "echo 'prettier --config .prettierrc.json --parser typescript --write \"./src/**/*.ts\";'",
      "lint:check": "echo 'prettier --config .prettierrc.json --parser typescript --check \"./src/**/*.ts\";'",
      "storybook:start": "start-storybook -p 6006",
      "storybook:build": "build-storybook",
      "lib:build:prod": "yarn prebuild && export NODE_ENV=production && rollup -c",
      "lib:build:analyze": "yarn build:prod --config-analyze"
  },
  "author": "akeneo",
  "license": "OSL-3.0",
  "dependencies": {
      "react": "^16.8.0",
      "react-dom": "^16.8.0",
      "styled-components": "^4.3.2"
  },
  "peerDependencies": {
      "styled-components": "^4.3.2"
  },
  "devDependencies": {
      "typescript": "^3.7.2"
  }
}
